{
  "hooks": {
    "SessionStart": [
      {
        "matcher": "startup",
        "hooks": [
          {
            "type": "command",
            "command": "\"$CLAUDE_PROJECT_DIR/bin/hook\""
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "\"$CLAUDE_PROJECT_DIR/bin/hook\""
          }
        ]
      }
    ]
  },
  "env": {
    "DISABLE_AUTOUPDATER": "1"
  },
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git log:*)",
      "Bash(git check-ignore:*)",
      "Bash(rg:*)",
      "Bash(ast-grep:*)",
      "Bash(ls:*)",
      "Bash(ln:*)",
      "Bash(mkdir:*)",
      "Bash(make:*)",
      "Bash(touch:*)",
      "Bash(uv sync:*)",
      "Bash(uv tree:*)",
      "Bash(jq:*)",
      "Bash(comrak:*)",
      "Bash(scpaste:*)",
      "Bash(yarn build:*)",
      "Bash(alacritty:*)",
      "Bash(npm test:*)"
    ]
  },
  "model": "opus",
  "alwaysThinkingEnabled": true,
  "feedbackSurveyState": {
    "lastShownTime": 1754368507584
  },
  "installMethod": "unknown"
}
